[user]
[push]
  default = simple
[core]
  autocrlf = input
  editor = vim
  quotepath = false
  pager = less -RS
[diff]
  tool = vimdiff
[merge]
  tool = vimdiff
[difftool]
  prompt = false
[alias]
  st = status
  b = branch
  pl = pull
  ps = push
  df = diff
  co = checkout
  commitam = commit --amend -C HEAD
  r = rebase
  l = log --pretty=oneline -n 20 --graph --abbrev-comit
  ls = log --oneline --decorate
  la = log --color --graph --online --decorate --all --
  pldev = pull --rebase origin dev
  pssu = push --set-upstream origin @{upstream}
  psf = push --force origin @{upstream}
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
